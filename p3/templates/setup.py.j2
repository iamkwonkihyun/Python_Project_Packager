from setuptools import setup, find_packages

setup(
    name="{{ name }}",
    version="{{ version }}",
    packages=find_packages(),
    install_requires=[
        {% for pkg in requirements %}
        "{{ pkg }}",
        {% endfor %}
    ],
    entry_points={
        "console_scripts": [
            "{{ cli_name }}={{ entry_point }}"
        ]
    },
)
